Algoritmo "FUNCAO_CALCULADORA"

Var

//A diferença entre função e procedimento
//é que a função tem um retorno
funcao somar(x, y : real) : real

inicio
  retorne x + y
fimfuncao

Inicio
  escreval("O resultado é: ",somar(2,6))
  escreval("O resultado é: ",somar(11,3))
  escreval("O resultado é: ",somar(4,12))
Fimalgoritmo